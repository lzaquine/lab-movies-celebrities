<h2>Celebrities:</h2>
<hr>
{{#each celebrities}}
<h3>Name: {{this.name}}</h3>
<h4>Occupation: {{this.occupation}}</h4>
<h4>Catch Phrase: {{this.catchPhrase}}</h4>
<form action="/celebrities/{{_id}}/delete" method="post">
<button type="submit">Delete</button>
</form>
<hr>
{{/each}}
<a href="/">Back</a>